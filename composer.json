{
    "name": "bravecollective/ping-app",
    "description": "",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Brave\\PingApp\\": "src"
        }
    },
    "repositories": [
        {
            "type": "git",
            "url": "https://github.com/bravecollective/web-ui.git"
        }
    ],
    "require": {
        "php": "^7.2",
        "ext-json": "*",
        "bravecollective/neucore-api": "^1.0",
        "bravecollective/web-ui": "*@dev",
        "bryanjhv/slim-session": "^4.1",
        "doctrine/dbal": "^2.10",
        "doctrine/orm": "^2.7",
        "frontpack/composer-assets-plugin": "^0.11.0",
        "php-di/php-di": "^6.3",
        "slim/psr7": "^1.4",
        "slim/slim": "^4.7",
        "tkhamez/eve-sso": "^1.0",
        "tkhamez/slim-role-auth": "^3.0"
    },
    "config": {
        "assets-dir": "web/assets",
        "platform": {
            "php": "7.2.0"
        },
        "sort-packages": true
    },
    "scripts": {
        "db:update": [
            "doctrine orm:schema-tool:update --force"
        ],
        "compile": [
            "doctrine orm:schema-tool:update --force"
        ]
    }
}
