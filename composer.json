{
    "name": "bravecollective/ping-app",
    "type": "project",
    "autoload": {
        "psr-4": {
            "Brave\\PingApp\\": "src"
        }
    },
    "repositories": [
    {
        "type": "git",
        "url": "https://github.com/bravecollective/web-ui.git"
    }
    ],
    "require": {
        "ext-json": "*",
        "bravecollective/neucore-api": "^1.0",
        "bravecollective/sso-basics": "^3.0",
        "bravecollective/web-ui": "*@dev",
        "bryanjhv/slim-session": "^3.5",
        "doctrine/dbal": "^2.10",
        "doctrine/orm": "^2.7",
        "frontpack/composer-assets-plugin": "^0.9",
        "tkhamez/slim-role-auth": "1.0"
    },
    "config": {
        "assets-dir": "web/assets",
        "platform": {
            "php": "7.2.0"
        },
        "sort-packages": true
    },
    "scripts": {
        "db:update": [
            "doctrine orm:schema-tool:update --force"
        ],
        "compile": [
            "doctrine orm:schema-tool:update --force"
        ]
    }
}
